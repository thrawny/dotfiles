{
  "editor.inlineSuggest.enabled": true,
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "vim.leader": ",",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "<leader>"
      ],
      "commands": [
        {
          "command": "workbench.action.openPreviousEditorFromHistory"
        },
        {
          "command": "workbench.action.acceptSelectedQuickOpenItem"
        }
      ]
    },
    {
      "before": [
        "<space>",
        "r",
        "r"
      ],
      "commands": [
        "editor.action.rename"
      ]
    },
    {
      "before": [
        "<space>",
        "p"
      ],
      "after": [
        "\"",
        "*",
        "p"
      ]
    },
    {
      "before": [
        "<leader>",
        "o"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "<C-l>"
      ],
      "commands": [
        "workbench.action.navigateRight"
      ]
    },
    {
      "before": [
        "<C-j>"
      ],
      "commands": [
        "workbench.action.navigateDown"
      ]
    },
    {
      "before": [
        "<C-h>"
      ],
      "commands": [
        "workbench.action.navigateLeft"
      ]
    },
    {
      "before": [
        "<C-k>"
      ],
      "commands": [
        "workbench.action.navigateUp"
      ]
    },
    {
      "before": [
        "j"
      ],
      "after": [
        "g",
        "j"
      ]
    },
    {
      "before": [
        "k"
      ],
      "after": [
        "g",
        "k"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<space>",
        "p"
      ],
      "after": [
        "\"",
        "*",
        "p"
      ]
    },
    {
      "before": [
        "<space>",
        "y"
      ],
      "after": [
        "\"",
        "*",
        "y"
      ]
    }
  ],
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "explorer.confirmDelete": false,
  "editor.formatOnSave": true,
  "[yaml]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "kennylong.kubernetes-yaml-formatter"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.tabSize": 2
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.fontSize": 13,
  "workbench.editor.showTabs": "single",
  "files.restoreUndoStack": false,
  "workbench.colorCustomizations": {
    "problemsErrorIcon.foreground": "#00000000"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.tabSize": 2
  },
  "typescript.tsdk": "./node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "cSpell.userWords": [
    "capalo",
    "kanel",
    "Kustomization",
    "kustomize",
    "mqtt",
    "opentelemetry",
    "otel",
    "OTLP",
    "SEMATTRS",
    "SEMRESATTRS",
    "Todos",
    "unnest",
    "zerolog"
  ],
  "tailwindCSS.includeLanguages": {
    "templ": "html"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "workbench.colorTheme": "Darcula",
  "editor.fontFamily": "Source Code Pro For Powerline",
  "editor.fontSize": 13,
  "go.toolsManagement.autoUpdate": true,
  "editor.snippetSuggestions": "none",
  "typescript.suggest.objectLiteralMethodSnippets.enabled": false,
  "typescript.suggest.classMemberSnippets.enabled": false,
  "javascript.suggest.classMemberSnippets.enabled": false,
  "search.useIgnoreFiles": true,
  "search.smartCase": true,
  "search.useGlobalIgnoreFiles": true,
  "workbench.sideBar.location": "left",
  "explorer.confirmDragAndDrop": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "hediet.vscode-drawio.resizeImages": null,
  "hediet.vscode-drawio.theme": "sketch",
  "editor.inlineSuggest.suppressSuggestions": true,
  "debug.onTaskErrors": "abort",
  "cursor.cpp.disabledLanguages": [
    "plaintext",
    "scminput"
  ],
  "cSpell.enabled": false,
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always",
      "source.unusedImports": "always"
    },
    "diffEditor.ignoreTrimWhitespace": false
  },
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "git.openRepositoryInParentFolders": "never",
  "[sql]": {
    "editor.formatOnSave": false
  },
  "vs-kubernetes": {
    "vs-kubernetes.crd-code-completion": "enabled"
  },
  "jupyter.askForKernelRestart": false,
  "scratchpads.defaultFiletype": "csv",
  "rest-client.previewColumn": "current",
  "rest-client.showResponseInDifferentTab": true,
  "sqltools.highlightQuery": false,
  "[csv]": {
    "editor.inlayHints.maximumLength": 0
  },
  "[dynamic csv]": {
    "editor.inlayHints.maximumLength": 0
  },
  "jupyter.runStartupCommands": [],
  "cursor.general.globalCursorIgnoreList": [
    "**/.env",
    "**/.env.*",
    "**/credentials.json",
    "**/credentials.*.json",
    "**/secret.json",
    "**/secrets.json",
    "**/*.key",
    "**/*.pem",
    "**/*.pfx",
    "**/*.p12",
    "**/*.crt",
    "**/*.cer",
    "**/id_rsa",
    "**/id_dsa",
    "**/.ssh/id_*",
    "**/.envrc"
  ],
  "explorer.autoReveal": false,
  "git.autofetch": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.renderSideBySide": true,
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "git.branchProtectionPrompt": "alwaysCommit",
  "gitlens.graph.layout": "editor",
  "python.terminal.activateEnvironment": false
}
